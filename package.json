{
  "name": "@ski/ski",
  "version": "0.1.0",
  "description": "Bundle of all @ski packages",
  "main": "ski.js",
  "module": "ski.js",
  "types": "ski.d.ts",
  "exports": {
    "require": "./ski.cjs",
    "import": "./ski.js"
  },
  "scripts": {
    "build": "run-s clean build:*",
    "watch": "run-p \"build:* -- --watch\"",
    "clean": "rimraf js .types",
    "build:bundle": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saviski/ski.git"
  },
  "author": "saviski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/saviski/ski/issues"
  },
  "homepage": "https://github.com/saviski/ski#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.59.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.3",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "files": [
    ".types",
    "js",
    "src",
    "ski.js",
    "ski.min.js",
    "ski.min.js.map"
  ],
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "tabWidth": 2,
    "proseWrap": "never",
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "singleQuote": true
  }
}